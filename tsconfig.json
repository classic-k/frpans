{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
     "baseUrl":".",
    "paths": {
      "@/*": ["./src/*"],
    "@pancakeswap/localization":["/src/pancakeswap/localization/src"],
    "@pancakeswap/localization/*":["/src/pancakeswap/localization/*"],
     "@pancakeswap/utils/*":["/src/pancakeswap/utils/*"],
     "@pancakeswap/multicall":["/src/pancakeswap/multicall"],
     // "@pancakeswap/hooks":["/src/pancakeswap/hooks/src"],
     // "@pancakeswap/hooks/*":["/src/pancakeswap/hooks/src/*"],
     "@pancakeswap/uikit":["/src/pancakeswap/uikit/src"],
      //"utils":["/src/utils"],
      "utils/*":["/src/utils/*"],
      // "components":["/src/components"],
       "components/*":["/src/components/*"],
       //"hooks":["/src/hooks"],
       //"hooks/*":["/src/hooks/*"],
       //"@pancakeswap/multicall":["/src/pancakeswap/multicall"],
       //"@pancakeswap/multicall/*":["/src/pancakeswap/multicall/*"],
       //"hooks/useActiveChainId":["./src/hooks/useActiveChainId"],
       //"@pancakeswap/farms":["/src/pancakeswap/farms"],
       //"@pancakeswap/farms/constants/*":["/src/pancakeswap/farms/constants/*"],
       //"@pancakeswap/farms/constants/priceHelperLps":["/src/pancakeswap/farms/constants/priceHelperLps"],
      // "@pancakeswap/farms/*":["/src/pancakeswap/farms/*"],
      // "contexts":["/src/contexts"],
       //"contexts/*":["/src/contexts/*"],
       "@pancakeswap/ui-wallets":["/src/pancakeswap/ui-wallets/src"],
      "@pancakeswap/ui":["/src/pancakeswap/ui"],
       "@pancakeswap/ui/*" :["/src/pancakeswap/ui/*"],
       "@pancakeswap/uikit/*" :["/src/pancakeswap/uikit/src/*"],
      "@pancakeswap/wagmi/*":["/src/pancakeswap/wagmi/*"],
      "@pancakeswap/wagmi":["/src/pancakeswap/wagmi/src"],
      "@pancakeswap/hooks":["/src/pancakeswap/hooks/src"],
      "@pancakeswap/tokens":["/src/pancakeswap/tokens/src"],
      "@pancakeswap/swap-sdk-core":["/src/pancakeswap/swap-sdk-core/src"],
      "@pancakeswap/swap-sdk":["/src/pancakeswap/swap-sdk/src"],
       "views/*":["/src/views/*"],
       "contexts/*": ["/src/contexts/*"],
       "config/*":["/src/config/*"],
      "state/*":["/src/state/*"],
      "state":["/src/state"],
       "hooks/*":["/src/hooks/*"],
       "utils":["/src/utils"],
       "utils/*":["/src/utils/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules"]
}
